<!--components/swiping-gallery.wxml-->
<swiper class="-gallery" autoplay indicator-dots indicator-color="gray" circular >
  <swiper-item wx:key='index' wx:for="{{images}}">
    <navigator url="{{item.url}}">
      <image mode="aspectFill" src="{{item.image}}" style="width:100%;"></image>
    </navigator>
  </swiper-item>
</swiper>
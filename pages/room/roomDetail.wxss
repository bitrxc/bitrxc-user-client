/* pages/room/roomDetail.wxss */
.container{
  align-items: flex-start;
  text-align: left;
}
.swiper{
  position: relative;
  z-index: 100;
  border-radius: 10rpx;
  overflow: hidden;
}
.content{
  background-color: white;
  position:relative;
  z-index: 200;
  width: 94%;
  top: -20px;
  border-radius: 5px;
  margin: 0 auto;
  box-shadow: 0px 3px 9px #979696;
}
.roomname{
  font-size:30px;
  margin: 15px 15px 15px 15px ;
  color: rgb(112, 111, 111);
}
.title{
  font-size: 20px;
  margin: 10px 10px 10px 15px;
}
.small-title{
  font-size: 15px;
    margin: 10px 15px;
    color: rgb(150,150,150);
}
.small-content{
  color: rgb(50,50,50);
}
.line-top{
  border-top: outset rgb(240, 240, 240);
  }
.line-bottom{
border-bottom: inset rgb(240, 240, 240);
}
.img-content{
  display: flex;
  justify-items: center;
  align-content: center;
  margin:10px 10px 10px 0;
  width:fit-content;
}
.btn{
  margin: 35px;
}
.deal{
  display: flex;
  align-content: center;
  justify-content: center;
}
.deal-des{
  background-color: rgba(241, 100, 123, 0.931);
  border-radius: 5px;
  width: 50%;
  display: flex;
  align-content: center;
  justify-content: center;
  margin:0 0 20px 0px;
  color:white;
  font-size: 13px;
}